program test_math_func;

var
    a,b,d,e : integer;
    r,p : real;
    letter,ch : char;
    t,f : boolean;

begin
    a := abs(-157);
    writeln(a);

    b := sqr(5);
    p := sqrt(49.0);
    writeln(b," ",p);

    letter := chr(65);
    writeln(letter);

    t := odd(3);
    f := odd(2);
    writeln(t," ",f);

    d := ord('a');
    writeln(d);

    e := succ(87);
    writeln(e);
end.