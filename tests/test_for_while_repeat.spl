program test_for_while;

var
    t,s : real;
    i,j,n : integer;

begin
    s := 0;
    for n := 1 to 10 do
    begin
        t := 1;
        for i := 1 to n do
            t := t*i;
        s := s + t;
    end;
    writeln("result = ", s);

    s := 0;
    n := 1;
    while n <= 10 do
    begin
        j := 1;
        t := 1;
        repeat
        begin
            t := t*j;
            j := j+1;
        end;
        until j = n+1;
        s := s+t;
        n := n+1;
    end;
    writeln("result = ", s);
end.


